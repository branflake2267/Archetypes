#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD GWT 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="module">
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name="com.google.gwt.user.User"/>

  <!-- Inherit the default GWT style sheet.                       -->
  <!-- <inherits name="com.google.gwt.user.theme.clean.Clean"/> -->
  <!-- <inherits name="com.google.gwt.user.theme.standard.Standard"/> -->
  <!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
  <!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class="${package}.client.${module}EntryPoint" />

  <!-- Specify the paths for translatable code                    -->
  <source path="client"/>
  <source path="shared"/>

  <!-- CSS Resource Style options: obf,pretty,... -->
  <set-configuration-property name="CssResource.style" value="obf" />
  
</module>
